@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}
@model ZarenTravelProject.Models.ListedItemsResponseOperationResult?
@if (Model != null)
{
    <table class="table table-striped">
        <thead>
        <tr>
            <td>Location</td>
            <td>locationId</td>
            <td>Description</td>
            <td>Type</td>
            <td>Provider</td>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model.Data.Items)
        {
            <tr>
                <td>@item.Title</td>
                <td>@item.Value</td>
                <td>@item.Description</td>
                <td>@item.Type</td>
                <td>@item.Provider</td>
                <td>
                    <a class="btn btn-warning" asp-controller="Home" asp-action="SearchHotelWithLocation" asp-route-id="@item.Value">Hotel</a>
                </td>
            </tr>
        }
        </tbody>
    </table>
}